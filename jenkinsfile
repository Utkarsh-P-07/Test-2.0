

pipeline{
    agent any
    stages {
        stage('checkout'){
            steps{
                git url : 'https://github.com/Utkarsh-P-07/Test-2.0.git', branch : 'master'
            }
        }
        stage('build'){
            steps{
                echo 'building'
            }
        }
        stage('test'){
            steps{
                echo 'testing'
            }
        }
        stage('deploy'){
            steps{
                echo 'deploying'
            }
        }
    }
}
